<h2>Users Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td>
        <%= user.fullName %>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.zipCode %>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
<tbody>
 <% users.forEach(user => { %>
 <tr>
 <td><a href="/users/<%= user._id %>"><%= user.name %></a></td>
 <td><%= user.email %></td>
 <td><%= user.zipCode %></td>
 </tr>
 <% }) %>
</tbody>